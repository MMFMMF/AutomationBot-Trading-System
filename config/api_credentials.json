{
  "polygon_io": {
    "api_key": "${POLYGON_API_KEY}",
    "base_url": "https://api.polygon.io",
    "endpoints": {
      "quotes": "/v2/last/trade/{symbol}",
      "aggs": "/v2/aggs/ticker/{symbol}/prev",
      "market_status": "/v1/marketstatus/now",
      "ticker_details": "/v3/reference/tickers/{symbol}",
      "news": "/v2/reference/news"
    }
  },
  "tradestation": {
    "client_id": "${TRADESTATION_CLIENT_ID}",
    "client_secret": "${TRADESTATION_CLIENT_SECRET}",
    "base_url": "https://sim-api.tradestation.com/v3",
    "auth_url": "https://signin.tradestation.com/oauth/token",
    "endpoints": {
      "accounts": "/brokerage/accounts",
      "orders": "/brokerage/accounts/{account_id}/orders",
      "positions": "/brokerage/accounts/{account_id}/positions",
      "balances": "/brokerage/accounts/{account_id}/balances"
    }
  },
  "yahoo_finance": {
    "base_url": "https://query1.finance.yahoo.com",
    "endpoints": {
      "quote": "/v8/finance/chart/{symbol}",
      "news": "/v1/finance/search"
    }
  },
  "alpha_vantage": {
    "api_key": "${ALPHA_VANTAGE_API_KEY}",
    "base_url": "https://www.alphavantage.co",
    "endpoints": {
      "quote": "/query?function=GLOBAL_QUOTE&symbol={symbol}",
      "intraday": "/query?function=TIME_SERIES_INTRADAY&symbol={symbol}",
      "news": "/query?function=NEWS_SENTIMENT&tickers={symbol}"
    }
  },
  "defi": {
    "wallet_address": "${DEFI_WALLET_ADDRESS}",
    "private_key": "${DEFI_PRIVATE_KEY}",
    "oneinch_api_url": "https://api.1inch.io/v5.0/1",
    "simulation_mode": true,
    "endpoints": {
      "quote": "/quote",
      "swap": "/swap",
      "healthcheck": "/healthcheck"
    }
  }
}